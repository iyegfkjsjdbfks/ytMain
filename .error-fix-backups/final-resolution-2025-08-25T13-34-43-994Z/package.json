{
  "name": "typescript-error-resolution",
  "version": "1.0.0",
  "description": "Automated TypeScript error resolution system with intelligent categorization and bulk fixing capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "error-resolver": "dist/cli/main.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/error-resolution/cli/main.js",
    "dev": "vite",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit --pretty",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "error-resolution",
    "automation",
    "code-fixing",
    "compilation-errors",
    "developer-tools"
  ],
  "author": "TypeScript Error Resolution Team",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "clsx": "^2.1.1",
    "commander": "^11.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^29.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "tsx": "^4.20.4",
    "vite": "^7.1.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/typescript-error-resolution.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/typescript-error-resolution/issues"
  },
  "homepage": "https://github.com/your-org/typescript-error-resolution#readme"
}
